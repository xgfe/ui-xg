        "json-loader": "^0.5.7",
        "karma": "^0.13.16",
        "karma-chrome-launcher": "^0.2.2",
        "karma-coverage": "^1.0.0",
        "karma-jasmine": "^0.3.6",
        "karma-junit-reporter": "^0.3.8",
        "lint-plus": "^0.6.7",
        "loader-utils": "^1.1.0",
        "markdown-loader": "^5.1.0",
        "marked": "^0.3.5",
        "mini-css-extract-plugin": "^0.8.0",
        "raw-loader": "^3.1.0",
        "run-sequence": "^1.2.2",
        "sass-loader": "^7.2.0",
        "style-loader": "^0.21.0",
        "url-loader": "^1.1.2",
        "webpack": "^4.39.2",
        "webpack-cli": "^3.3.6",
        "webpack-dev-middleware": "^3.1.3",
        "webpack-dev-server": "^3.8.0",
        "xg-htmlhint": "0.0.9"
