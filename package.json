{
    "name": "ui-xg",
    "version": "2.1.22",
    "description": "UI directives based on angular and bootstrap",
    "main": "dist/js/ui-xg.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/xgfe/ui-xg.git"
    },
    "scripts": {
        "test": "gulp test",
        "postinstall": "bower install",
        "new": "node scripts/generators/index.js",
        "site:init": "node build/init-site/generator.js",
        "start": "npm run site:init && webpack-dev-server --config build/webpack.config.demo.js",
        "build:lib": "gulp",
        "build:demo": "npm run site:init && rm -rf publish && MODE=publish TARGET=github webpack --config build/webpack.config.demo.js",
        "build": "npm run build:lib && npm run build:demo"
    },
    "keywords": [
        "UI",
        "angular",
        "angular directives",
        "bootstrap"
    ],
    "author": "xgfe",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "angular": "^1.3.20",
        "angular-animate": "^1.3.20",
        "angular-mocks": "^1.3.20",
        "angular-sanitize": "^1.3.20",
        "angular-ui-router": "^0.4.2",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.6",
        "bootstrap": "^3.2.0",
        "css-loader": "^1.0.1",
        "cz-conventional-changelog": "^1.2.0",
        "ejs": "^2.4.1",
        "eslint-plugin-angular": "^3.0.0",
        "express": "^4.16.3",
        "file-loader": "^4.2.0",
        "font-awesome": "^4.1.0",
        "ghooks": "^2.0.4",
        "glob": "^6.0.3",
        "gulp": "^3.9.0",
        "gulp-angular-html2js": "0.0.1",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^2.0.7",
        "gulp-concat": "^2.6.0",
        "gulp-conventional-changelog": "^1.1.0",
        "gulp-eslint": "^5.0.0",
        "gulp-insert": "^0.5.0",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "^0.5.4",
        "gulp-rimraf": "^0.2.0",
        "gulp-sass": "^4.0.1",
        "gulp-uglify": "^1.5.1",
        "gulp-watch": "^5.0.0",
        "highlight.js": "^9.1.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^5.5.0",
        "jasmine-core": "^2.4.1",
